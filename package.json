{
  "name": "rutherford-scattering",
  "version": "1.2.0-dev.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/rutherford-scattering.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "RUTHERFORD_SCATTERING",
    "phetLibs": [
      "shred"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
