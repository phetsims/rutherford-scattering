    "simulation": true,
    "supportsOutputJS": true,
    "simFeatures": {
      "colorProfiles": [
        "default",
        "projector"
      ]
    },
    "published": true
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "rules": {
      "todo-should-have-issue": "off"
    }
  }
}